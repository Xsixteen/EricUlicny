<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="">
  <meta name="author" content="Eric Ulicny">

  <title>{{ title }}</title>

  {% block stylesheets %}
    <link rel="stylesheet" href="./css/bootstrap.min.css">
    <link rel="stylesheet" href="./css/main.css">
    <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Leckerli+One" />
  {% endblock %}
</head>

<body>

<!-- navigation -->
{% include 'fragments/navigation.twig' %}
<!-- /navigation -->

<!-- content -->
<div class="pre-content">
  {% block precontent %} {% endblock %}
</div>

<div class="main-content">
  {% block content %} {% endblock %}
</div> <!-- /main-content -->

{% block footer %}
  <footer class="footer" style="padding-top: 15px;">
    <div class="container">
      <div class="row">
        <div class="col-md-8 col-xs-12">
          <h4>Stub!</h4>
        </div>
      </div>
    </div>
  </footer>
{% endblock %}

{% block scripts %}
  <script type="application/javascript" src="https://code.jquery.com/jquery-2.2.4.min.js"   integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44="   crossorigin="anonymous"></script>
  <script type="application/javascript" src="./js/bootstrap.min.js"></script>
{% endblock %}
</body>
</html>